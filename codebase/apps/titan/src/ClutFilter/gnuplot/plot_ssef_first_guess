set title "SSEF on reconstructed spectrum, first guess"
set xlabel "sample"
set ylabel "Power (dB)" 
set xrange [0:63]
set yrange [1.e-10:10]
set logscale y
plot 'ssef_notched_0.real' title 'Notched' with lines, 'ssef_unfiltered.real' title 'Combined' with lines, 'ssef_wx_truth.real' title 'Wx truth' with lines, 'ssef_gaussian_0.real' title 'Fit'


