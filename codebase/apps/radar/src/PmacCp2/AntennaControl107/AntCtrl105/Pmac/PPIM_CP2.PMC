p229=p(pfixang+29)+p(volpnt+veloff)
p230=p(pfixang+30)+p(volpnt+veloff)
p231=p(pfixang+31)+p(volpnt+veloff)
p232=p(pfixang+32)+p(volpnt+veloff)
p233=p(pfixang+33)+p(volpnt+veloff)
p234=p(pfixang+34)+p(volpnt+veloff)
