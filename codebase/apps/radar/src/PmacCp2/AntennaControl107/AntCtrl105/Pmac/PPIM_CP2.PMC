volcnt=volcnt+1                 ;inc volume count
swpcnt=swpcnt+1                 ;inc sweep count
fixed_ang=p(pfixang)+p52           ;initialize fixed_ang
elpnt=p(pfixang)+p52               ;send el to starting point
