p223=p(pfixang+23)+p(volpnt+veloff)
p224=p(pfixang+24)+p(volpnt+veloff)
p225=p(pfixang+25)+p(volpnt+veloff)
p226=p(pfixang+26)+p(volpnt+veloff)
p227=p(pfixang+27)+p(volpnt+veloff)
