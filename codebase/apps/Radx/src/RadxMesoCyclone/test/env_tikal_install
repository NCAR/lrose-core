setenv BIN_DIR          $PWD/../../ncar/rap/bin

#
# where is CIDD?
#
setenv DISPLAY_BIN_DIR /opt/local/lrose/bin


#
#  Incoming Radar data
#
setenv DATA_URL  'mdvp:://hail.eol.ucar.edu::/scr/hail1/rsfdata/eolbase/cfradial/kftg/moments'

#
#  Local top level data path and host
#
setenv RAP_DATA_DIR     $PWD/../data_dir
