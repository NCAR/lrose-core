// 1D array - variable length.
//

fixed_const = {
};

///////////// user_data ///////////////////////////////
//
// Non gridded data names.
//
//
// Type: string
// 1D array - variable length.
//

user_data = {
};

///////////// volume_before_filter ////////////////////
//
// Volume Filters to apply before the sweep filters.
//
//
// Type: string
// 1D array - variable length.
//

volume_before_filter = {
};

///////////// filter //////////////////////////////////
//
// Filters (sweep) to apply.
//
//
// Type: string
// 1D array - variable length.
//

filter = {
 "MedDbz = median(DBZ, 10, 10, 0, 80, 5)",
 "FiltDbz = mask(MedDbz, 0, 25)",
 "ExpandDbz = Expand(FiltDbz, 20)",
 "FiltVel = mask_missing_to_missing(VEL, FiltDbz)",
 "Shear1 = AzShear(VEL, 1, 1)",
 "Shear3 = AzShear(VEL, 3, 1)",
 "Shear5 = AzShear(VEL, 5, 1)",
 "Shear10 = AzShear(VEL, 10, 1)",
 "Shear21 = AzShear(VEL, 1, 0)",
 "Shear23 = AzShear(VEL, 3, 0)",
 "Shear25 = AzShear(VEL, 5, 0)",
 "Shear210 = AzShear(VEL, 10, 0)",
 "ShearMax = maximum(Shear1, Shear3, Shear5, Shear10)",
 "Shear2Max = maximum(Shear21, Shear23, Shear25, Shear210)",
 "ShearMedian = median(ShearMax, 4, 4, 0, 30, 3)",
 "Shear2Median = median(Shear2Max, 4, 4, 0, 30, 3)",
 "ShearThresh = mask(ShearMedian, -100, 10)",
 "Shear2Thresh = mask(Shear2Median, -100, 10)",
 "ShearClump = clump(ShearThresh, 10)",
 "Shear2Clump = clump(Shear2Thresh, 10)",
 "Max = max_expand(ShearClump, 2, 2)",
 "Max2 = max_expand(Shear2Clump, 2, 2)",
 "Meso1 = MesoTemplate(FiltVel, 20, 20, 10)",
 "Meso2 = MesoTemplate(FiltVel, 10, 10, 5)"
};

///////////// volume_after_filter /////////////////////
//
// Volume Filters to apply after the sweep filters.
//
//
// Type: string
// 1D array - variable length.
//

volume_after_filter = {
  "Vertical =  VerticalConsistency(Max)",
  "Vertical2 =  VerticalConsistency(Max2)"
};

///////////// min_vel_diff ////////////////////////////
//
// Minimum diff in velocities between towards and away.
//
//
// Type: double
//

min_vel_diff = 10;

///////////// min_percent_large ///////////////////////
//
// Minimum percentage of away or towards data that have large values.
//
// Large means at least half as big as the average.
//
//
// Type: double
//

min_percent_large = 0.5;

///////////// min_percent_good ////////////////////////
//
// Minimum percentage of data points in the template that are good.
//
//
// Type: double
//

min_percent_good = 0.6;

