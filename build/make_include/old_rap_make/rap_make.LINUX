SYS_X_LIBS = -lXext -lXt -lX11 -lSM -lICE

SYS_LDFLAGS = $(MY_LDFLAGS) "-Wl,-rpath,$(RAP_LIB_DIR)"
SYS_CPPC_LDFLAGS = $(MY_CPPC_LDFLAGS) "-Wl,-rpath,$(RAP_LIB_DIR)"

SYS_X_LDFLAGS = -L/usr/X11R6/lib
SYS_F_LDFLAGS =
SYS_XVIEW_LDFLAGS =
SYS_MOTIF_LDFLAGS = 
SYS_MOTIF_LIBS = -lXm -lXp -lXext 
LEX_LIB = -ll
YACC_LIB =
SYS_UCB_LIBS =
SYS_UCB_LDFLAGS =

